<Window x:Class="AST_Code_Generation.View.ObservedGaussianWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AST_Code_Generation.View"
        mc:Ignorable="d"
        Title="ObservedGaussianWindow" Height="200" Width="200">
    <StackPanel>
        <StackPanel>
            <Label x:Name="Title" Content="variable name: " FontWeight="Bold"></Label>

            <TextBox x:Name="Value1" Margin="10" Width="100" HorizontalAlignment="Left"></TextBox>

            <Label x:Name="ObservedValue" Content="Observed Value: " FontWeight="Bold"></Label>

            <TextBox x:Name="Value2" Margin="10" Width="100" HorizontalAlignment="Left"></TextBox>

            <Button x:Name="B" Width="100" Margin="10" HorizontalAlignment="Left" Content="Add Parameters" Click="B_Click"></Button>
        </StackPanel>
    </StackPanel>
</Window>
